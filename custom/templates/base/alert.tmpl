{{if .Flash.ErrorMsg}}
	<div class="alert alert-danger">
		{{.Flash.ErrorMsg | Str2html}}
	</div>
{{end}}
{{if .Flash.SuccessMsg}}
	<div class="alert alert-success">
		{{.Flash.SuccessMsg | Str2html}}
	</div>
{{end}}
{{if .Flash.InfoMsg}}
	<div class="alert alert-info">
		{{.Flash.InfoMsg | Str2html}}
	</div>
{{end}}
